{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/data": [
        "src/data/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}